programa {
  funcao inicio() {


    real altura,peso,massacorporal,arrendoda

    escreva("Digite sua altura: ")
    leia(altura)

    escreva("Digite seu peso: ")
    leia(peso)

    massacorporal=peso/(altura*altura)
   
